<?xml version="1.0" encoding="UTF-8"?>

<routes>

    <vType id="car" accel="1.0" decel="4.5" sigma="0.5"
           length="5" maxSpeed="13.9"/>

    <!-- Routes -->
    <route id="n2s" edges="north_in south_out"/>
    <route id="s2n" edges="south_in north_out"/>
    <route id="e2w" edges="east_in west_out"/>
    <route id="w2e" edges="west_in east_out"/>

    <!-- Continuous traffic -->
    <flow id="flow1" type="car" route="n2s" begin="0" end="1000" vehsPerHour="600"/>
    <flow id="flow2" type="car" route="s2n" begin="0" end="1000" vehsPerHour="600"/>
    <flow id="flow3" type="car" route="e2w" begin="0" end="1000" vehsPerHour="600"/>
    <flow id="flow4" type="car" route="w2e" begin="0" end="1000" vehsPerHour="600"/>

</routes>
